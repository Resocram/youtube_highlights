{
    "manifest_version": 3,
    "name": "Highlight Commenter",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "http://www.youtube.com/watch*",
                "https://www.youtube.com/watch*"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "smiskiCheering.png"
    }
}